class Solution:
    # @param A : tuple of integers
    # @return a strings
    def largestNumber(self, A):
        for i in range(len(A)-1):
            A[i+1] = self.compare(str(A[i]), str(A[i+1]))
        return A[-1]
        
    def compare(self, x, y):
        return (x+y) if int(x+y) > int(y+x) else (y+x)


a = Solution()
print(a.largestNumber((0,0,0,0,0))
